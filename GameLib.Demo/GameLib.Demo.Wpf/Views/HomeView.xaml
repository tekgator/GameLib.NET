<UserControl
    x:Class="GameLib.Demo.Wpf.Views.HomeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:GameLib.Demo.Wpf.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Background="{StaticResource PrimaryBackgroundColor}"
    FontSize="16"
    Foreground="{StaticResource PrimaryForegroundColor}"
    mc:Ignorable="d">
    <wv2:WebView2 Source="https://github.com/tekgator/GameLib.NET/blob/main/README.md" />
</UserControl>
